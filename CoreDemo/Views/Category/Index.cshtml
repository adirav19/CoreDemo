@using EntityLayer.Concrete
@model List<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table>
    <tr>
        <th>Kategori ID</th>
        <th>Kategori Adı</th>
        <th>Durum</th>
        <th>Detaylar</th>
        <th>sil</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
            <td>@item.CategoryStatus</td>
            <td>Detayar</td>
            <td><button>sil</button></td>
        </tr>
    }

</table>

